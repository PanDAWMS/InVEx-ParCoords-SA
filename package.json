{
  "name": "@PanDAWMS/invex-parallel-coordinates",
  "version": "1.0.3",
  "description": "A graph with parallel coordinates and connected table",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prepare-js": "cat parcoords/js/ParallelCoordinates.js > index.js && echo '\nexport default ParallelCoordinates;' >> index.js",
    "prepare-css": "cat parcoords/css/ParallelCoordinates.css > ParallelCoordinates.css",
    "prepare-assets": "npm run -s prepare-js & npm run -s prepare-css",
    "submodule-update": "git clone -b master git://github.com/PanDAWMS/InVEx-ParCoords-SA.git parcoords",
    "install": "npm run -s submodule-update && npm run -s prepare-assets"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/PanDAWMS/InVEx-ParCoords-SA.git"
  },
  "keywords": [
    "visualization",
    "graph"
  ],
  "author": "archv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PanDAWMS/InVEx-ParCoords-SA/issues"
  },
  "homepage": "https://github.com/PanDAWMS/InVEx-ParCoords-SA#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "datatables.net-buttons-jqui": "^1.6.1",
    "datatables.net-colreorder-jqui": "^1.5.2",
    "datatables.net-fixedcolumns-jqui": "^3.3.0",
    "datatables.net-fixedheader-jqui": "^3.1.6",
    "datatables.net-jqui": "^1.10.20",
    "datatables.net-responsive-jqui": "^2.2.3",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.1",
    "select2": "^4.0.13",
    "simple-statistics": "^7.0.8"
  }
}
