{
  "name": "@PanDAWMS/invex-parallel-coordinates",
  "version": "1.0.5",
  "description": "A graph with parallel coordinates and connected table",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prepare-js": "cat imports parcoords/ParallelCoordinates.js > src/index.js && echo '\nexport default ParallelCoordinates;' >> src/index.js",
    "prepare-css": "cat parcoords/ParallelCoordinates.css > ParallelCoordinates.css",
    "prepare-images": "cp parcoords/loading.gif loading.gif",
    "prepare-assets": "npm run -s prepare-js & npm run -s prepare-css & npm run -s prepare-images",
    "submodule-update": "rm -rf parcoords && mkdir -p src && git clone -b master git://github.com/PanDAWMS/InVEx-ParCoords-SA.git parcoords",
    "install": "npm run -s submodule-update && npm run -s prepare-assets && if [ ! -d 'node_modules/' ]; then npm install; fi",
    "postinstall": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/PanDAWMS/InVEx-ParCoords-SA.git"
  },
  "keywords": [
    "visualization",
    "graph"
  ],
  "author": "archv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PanDAWMS/InVEx-ParCoords-SA/issues"
  },
  "homepage": "https://github.com/PanDAWMS/InVEx-ParCoords-SA#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "d3": "^3.5.17",
    "datatables.net": "1.10.20",
    "datatables.net-buttons-jqui": "^1.6.1",
    "datatables.net-colreorder-jqui": "^1.5.2",
    "datatables.net-fixedcolumns-jqui": "^3.3.0",
    "datatables.net-fixedheader-jqui": "^3.1.6",
    "datatables.net-jqui": "1.10.20",
    "datatables.net-responsive-jqui": "^2.2.3",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "js-cookie": "^2.2.1",
    "select2": "^4.0.13",
    "simple-statistics": "^7.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.10.4",
    "@babel/plugin-transform-react-inline-elements": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^4.2.1",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "extract-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-transform-hmr": "^1.0.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
